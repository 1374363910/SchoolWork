"use strict";$(function(){$(".tag_row").on("click",".tag_item",function(){$(".load_more").hide(),$(".tag_item").removeClass("tag_choose"),$(this).addClass("tag_choose"),$(".tag_choose_txt").text($(this).text()),$("#choose_show").addClass("visibility_show"),$("#choose_show").removeClass("visibility_hide"),$("#tags_show").hide(),-1!=window.location.href.indexOf("column")?$(window).scrollTop(400):$(window).scrollTop(0),pageIndex=2,tagid=$(this).data("tid"),tagName=$(this).data("tname"),tagRequest(tagName)}),$(window).scroll(function(){$(window).scrollTop()>1350?$(".vip_float").show():$(".vip_float").hide()})});